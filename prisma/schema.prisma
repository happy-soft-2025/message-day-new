generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Image {
  id         Int  @id @default( autoincrement() )
  path       String
  category   String
}

model category {
  id         Int @id @default( autoincrement() )
  category   String @unique 
}

model messageNow {
  id        Int @id @default( autoincrement() )
  message   String
}

model message {
  id        Int @id @default( autoincrement() )
  category  String
  message   String
}